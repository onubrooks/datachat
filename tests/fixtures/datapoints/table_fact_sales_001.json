  "name": "Fact Sales Table",
  "table_name": "analytics.fact_sales",
  "schema": "analytics",
  "business_purpose": "Central fact table for all sales transactions across regions",
  "key_columns": [
    {
      "name": "sale_id",
      "type": "BIGINT",
      "business_meaning": "Unique identifier for each sale",
      "nullable": false
    },
    {
      "name": "amount",
      "type": "DECIMAL(18,2)",
      "business_meaning": "Transaction value in USD",
      "nullable": false
    },
    {
      "name": "sale_date",
      "type": "DATE",
      "business_meaning": "Date when the sale occurred",
      "nullable": false
    },
    {
      "name": "region_id",
      "type": "INTEGER",
      "business_meaning": "Foreign key to region dimension",
      "nullable": true
    }
  ],
  "relationships": [
