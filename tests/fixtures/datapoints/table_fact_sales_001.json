      "join_column": "product_id",
      "cardinality": "N:1"
    }
  ],
  "common_queries": [
    "SELECT SUM(amount) FROM analytics.fact_sales WHERE sale_date >= '2024-01-01'",
    "SELECT region_id, SUM(amount) FROM analytics.fact_sales GROUP BY region_id"
